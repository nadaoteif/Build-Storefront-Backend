{
  "name": "storefront-backend",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && nodemon dist/server.js",
    "build": "npm run clean && npx tsc",
    "clean": "rimraf ./dist",
    "watch": "npm run build && tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "create_TEST_DB": "db-migrate db:create shopping_test",
    "delete_TEST_DB": "db-migrate db:drop shopping_test",
    "migrate_up": "npx db-migrate up",
    "test": "set My_ENV=test&& tsc && db-migrate -e test up && jasmine && db-migrate -e test reset"
  },
  "author": "NadaOteif",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "clean": "^4.0.2",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "docker-compose": "^0.23.17",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npx": "^10.2.2",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "psql": "^0.0.1",
    "typescript": "^4.8.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.27.0",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.3.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2"
  },
  "keywords": []
}
